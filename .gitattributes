# Text files
*.txt     text
*.ini     text
*.log     text

# Unreal Engine asset files
*.uasset  -crlf
*.umap    -crlf
# Binary files
*.exe     binary
*.dll     binary
*.so      binary
*.dylib   binary
# Ignore Unreal Engine metadata files
*.uproject -text
*.uprojectdirs -text
*.sln -text
*.csproj -text
*.suo -text
*.vcxproj -text
*.vcxproj.filters -text
*.xcworkspace -text
*.xcuserdatad -text
*.uasset filter=lfs diff=lfs merge=lfs -text
*.umap filter=lfs diff=lfs merge=lfs -text
# Unreal Engine metadata files
*.uproject filter=lfs diff=lfs merge=lfs -text
*.uprojectdirs filter=lfs diff=lfs merge=lfs -text
*.sln filter=lfs diff=lfs merge=lfs -text
*.csproj filter=lfs diff=lfs merge=lfs -text
*.suo filter=lfs diff=lfs merge=lfs -text
*.vcxproj filter=lfs diff=lfs merge=lfs -text
*.vcxproj.filters filter=lfs diff=lfs merge=lfs -text
*.xcworkspace filter=lfs diff=lfs merge=lfs -text
*.xcuserdatad filter=lfs diff=lfs merge=lfs -text
path filter=lfs diff=lfs merge=lfs -text
*.uasset999 filter=lfs diff=lfs merge=lfs -text

#Added for LFS
# UE file types
*.uasset filter=lfs diff=lfs merge=lfs -text
*.umap filter=lfs diff=lfs merge=lfs -text

# Raw Content types
*.fbx filter=lfs diff=lfs merge=lfs -text
*.3ds filter=lfs diff=lfs merge=lfs -text
*.psd filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
*.xcf filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text